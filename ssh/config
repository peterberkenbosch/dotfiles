Host github.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa
Host macmini
 HostName 192.168.99.10
 ForwardAgent yes
Host *
	StrictHostKeyChecking ask
	VerifyHostKeyDNS ask
	NoHostAuthenticationForLocalhost yes
	IdentityFile ~/.ssh/id_rsa
	UseKeychain yes
	AddKeysToAgent yes
	ControlMaster auto
	ControlPath /tmp/ssh-%r@%h:%p.socket
